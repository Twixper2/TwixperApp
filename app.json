{
  "expo": {
    "name": "Twixper",
    "slug": "TwixperApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1b2a33"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "backgroundColor": "#1b2a33",
      "bundleIdentifier": "com.dinshdani.TwixperApp"
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "backgroundColor": "#1b2a33",
      "package": "com.dinshdani.TwixperApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
		"backgroundColor": "#1b2a33",
		"plugins": [
			[
				"expo-notifications",
				{
					"icon": "./local/assets/icon.png",
					"color": "#ffffff"
				}
			]
		]
	}
}
