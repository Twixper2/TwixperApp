{
	"expo": {
		"name": "Twixper",
		"slug": "TwixperApp",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"buildNumber": "1.0.0",
			"supportsTablet": true,
			"backgroundColor": "#1b2a33"
		},
		"android": {
			"versionCode": 1,
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"backgroundColor": "#1b2a33"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"backgroundColor": "#1b2a33"
	}
}